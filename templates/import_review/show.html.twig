{% extends 'base.html.twig' %}

{% block title %}{{ parent() }} - {{ title|default(' Viewer') }}{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {#	{{ encore_entry_link_tags('') }}#}
{% endblock %}

{% block body %}
    <h2 class="h3 mb-3 font-weight-normal">{{ title|default(' Viewer') }}</h2>
    <h2>Imported Owner Information</h2>







    <table class="table table-striped table-sm small">
        <tbody>
        <tr>
            <th class="text-nowrap">Association:</th>
            <td class="text-nowrap">{{ association.nameFormal }}</td>
        </tr>
        <tr>
            <th class="text-nowrap">Account Numbers:</th>
            <td>{{ include('import_review/blocks/account_numbers.html.twig') }}</td>
        </tr>
        <tr>
            <th>Login Credentials:</th>
            <td>{{ include('import_review/blocks/login_credentials.html.twig') }}</td>
        </tr>
        <tr>
            <th>Owner Name:</th>
            <td>{{ include('import_review/blocks/owner_details.html.twig') }}</td>
        </tr>
        <tr>
            <th>Phone Numbers:</th>
            <td>{{ include('import_review/blocks/phone_numbers.html.twig') }}</td>
        </tr>
        <tr>
            <th>Email Address:</th>
            <td>{{ include('import_review/blocks/email_address.html.twig') }}</td>
        </tr>
        <tr>
            <th>Property Address:</th>
            <td>{{ include('import_review/blocks/property_address.html.twig') }}</td>
        </tr>
        <tr>
            <th>Mailing Address:</th>
            <td>{{ include('import_review/blocks/mailing_address.html.twig') }}</td>
        </tr>
        <tr>
            <th>Legal Description:</th>
            <td>{{ include('import_review/blocks/legal_description.html.twig') }}</td>
        </tr>
        <tr>
            <th>Ownership Start Date:</th>
            <td>{{ import.ownershipStartDate }}</td>
        </tr>
        <tr>
            <th>Upload Date:</th>
            <td>{{ import.createdAt ? import.createdAt|date('Y-m-d H:i:s') : '' }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('import_index', {'upload_id': upload_id}) }}"
       class="btn btn-primary btn-lg btn-block active" role="button" aria-pressed="true">Return to List</a>

    <a href="{{ path('import_edit', {'importId': import.importId}) }}"
       class="btn btn-primary btn-lg btn-block active mb-2" role="button"
       aria-pressed="true">Edit this Record</a>





    {{ include('import/_delete_form.html.twig') }}
{% endblock %}
