<form method="post" action="{{ path('upload_delete', {'uploadId': upload.uploadId}) }}"
      onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ upload.uploadId) }}">
    <button class="btn btn-danger btn-lg btn-block active mt-2">Delete Record</button>
</form>
