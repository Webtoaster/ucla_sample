<form action="{{ path('app_login') }}" method="post" class="form-signin">
    <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1>
    {% if error %}
        <div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}
    {{ include('forms/blocks/Login.html.twig') }}
    <p class="mt-1 mb-1 text-muted text-nowrap">
        <a href="{{ url('app_home') }}" class="btn btn-primary btn-lg btn-block active" role="button"
           aria-disabled="true">Return to Home Page</a>
    </p>
    {{ include('blocks/copyright_footer.twig') }}</form>{#<form class="form-signin">#}
