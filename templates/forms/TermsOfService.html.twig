<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            {{ form_start(form) }}
            <fieldset>
                <legend class="legend1">The Legal Mumbo Jumbo.</legend>
                <div>
                    <iframe src="/tou.html"
                            height="500"
                            width="500"
                            style="background: #FFF;border:2px solid #000000;float:middle;"
                            class="frame-class"
                            id="terms_of_service" name="tos"></iframe>
                </div>
                <div class="form-group row">
                    <div class="col-sm-2"></div>
                    <div class="col-sm-10">
                        Link to Terms of Use.<br/>
                        {{ form_row(form.agreeTerms) }}
                    </div>
                </div>
            </fieldset>

            {{ include('forms/blocks/ButtonSave.html.twig') }}
            {{ form_rest(form) }}
            {{ form_end(form) }}
        </div>
    </div>
</div>

