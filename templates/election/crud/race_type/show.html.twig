{% extends 'base.html.twig' %}

{% block title %}RaceType{% endblock %}

{% block body %}
    <h1>RaceType</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>RaceTypeId</th>
            <td>{{ race_type.raceTypeId }}</td>
        </tr>
        <tr>
            <th>DescriptionShort</th>
            <td>{{ race_type.descriptionShort }}</td>
        </tr>
        <tr>
            <th>DescriptionLong</th>
            <td>{{ race_type.descriptionLong }}</td>
        </tr>
        <tr>
            <th>DisplayDescriptionLong</th>
            <td>{{ race_type.displayDescriptionLong ? 'Yes' : 'No' }}</td>
        </tr>
        <tr>
            <th>SortOrder</th>
            <td>{{ race_type.sortOrder }}</td>
        </tr>
        <tr>
            <th>CreatedFromIp</th>
            <td>{{ race_type.createdFromIp }}</td>
        </tr>
        <tr>
            <th>UpdatedFromIp</th>
            <td>{{ race_type.updatedFromIp }}</td>
        </tr>
        <tr>
            <th>CreatedAt</th>
            <td>{{ race_type.createdAt ? race_type.createdAt|date('Y-m-d H:i:s') : '' }}</td>
        </tr>
        <tr>
            <th>UpdatedAt</th>
            <td>{{ race_type.updatedAt ? race_type.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
        </tr>
        <tr>
            <th>IsActive</th>
            <td>{{ race_type.isActive ? 'Yes' : 'No' }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('race_type_index') }}" class="btn btn-primary btn-lg btn-block active" role="button"
       aria-disabled="true">Return to List</a>


    <a href="{{ path('race_type_edit', {'raceTypeId': race_type.raceTypeId}) }}"
       class="btn btn-danger btn-lg btn-block active" role="button" aria-disabled="true">Edit Record</a>

    {{ include('race_type/_delete_form.html.twig') }}
{% endblock %}
